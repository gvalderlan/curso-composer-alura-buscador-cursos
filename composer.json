{    "name": "gvalderlan/buscador-cursos",    "description": "Projeto que busca os curso no site da Alura",    "type": "library",    "authors": [        {            "name": "GValderlan",            "email": "gvalderlan@gmail.com"        }    ],    "require": {        "guzzlehttp/guzzle": "^7.4",        "symfony/dom-crawler": "^5.4.9",        "symfony/css-selector": "^5.4"    },    "autoload": {        "files": [            "./functions.php"        ],        "psr-4": {            "Alura\\BuscadorDeCursos\\": "src/"        }    },    "require-dev": {        "phpunit/phpunit": "9",        "squizlabs/php_codesniffer": "^3.7",        "phan/phan": "^5.3"    },    "scripts": {        "test": "phpunit tests\\TestBuscadorDeCursos.php",        "cs": "phpcs --standard=PSR12 src/",        "phan": "phan --allow-polyfill-parser",        "check": [            "@phan",            "@cs",            "@test"        ],        "post-update-cmd": [            "@test"        ]    },    "scripts-descriptions": {        "check": "Rodas as verificações do código PHAN, PHPCS e PHPUNIT"    }}